<configuration>
	<include
		resource="org/springframework/boot/logging/logback/defaults.xml" />

	<appender name="STDOUT"
		class="ch.qos.logback.core.ConsoleAppender"> 
		<encoder>
			<pattern>${CONSOLE_LOG_PATTERN}</pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>